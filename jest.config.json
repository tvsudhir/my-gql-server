{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js, ts}",
    "!**/node_modules/**",
    "!**/coverage/**",
    "!**/src/schema/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": -10
    }
  },
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  }
}